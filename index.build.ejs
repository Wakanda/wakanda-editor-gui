<html>

<head>
  <link rel="stylesheet" href="./lib/jstree/themes/default/style.css" type="text/css">

  <script src="lib/jquery-2.1.3.min.js"></script>

  <link rel="stylesheet" type="text/css" href="lib/bootstrap/bootstrap.min.css">

  <script src="lib/bootstrap/bootstrap.min.js"></script>

  <script src="lib/bootbox/bootbox.min.js"></script>

  <!-- Latest compiled and minified CSS -->
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">

  <!-- Optional theme -->
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap-theme.min.css">

  <!-- Latest compiled and minified JavaScript -->
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>

  <link rel="stylesheet" href="lib/bootstrap-vertical-tabs/bootstrap.vertical-tabs.min.css">
  <link rel="stylesheet" href="lib/awesomplete/awesomplete.css">
</head>

<body>
  <div class="page_ribbon">
    <img class="page_ribbon_logo" src="./images/logo.png">
  </div>
  <div class="page_toolbar"></div>

  <div id="side-panel">
    <div class="col-xs-2" id="tabs-container" class="cloud-ide-tree unselectable">
      <ul class="nav nav-tabs tabs-left" id="tabs-list">
        <li>
          <a data-toggle="tab" href="#outline">Outline</a>
        </li>
        <li>
          <a data-toggle="tab" href="#angular">Angular</a>
        </li>
        <li class="active">
          <a data-toggle="tab" href="#styling">Styling</a>
        </li>
        <li>
          <a data-toggle="tab" href="#components">Components</a>
        </li>
      </ul>
      <ul id="responsiveButtonsList" class="nav nav-tabs tabs-left">
      </ul>
    </div>

    <div class="tab-content col-xs-10">
      <div class="cloud-ide-tree unselectable tab-pane" id="tree"></div>
      <div class="cloud-ide-tree unselectable tab-pane" id="outline"></div>
      <div class="cloud-ide-tree unselectable tab-pane" id="angular"></div>
      <div class="cloud-ide-tree unselectable tab-pane active" id="styling">
        <h3>Style</h3>
      </div>
      <div class="cloud-ide-tree unselectable tab-pane" id="components"></div>
    </div>
  </div>


  <!-- <div class="cloud-ide-tree-editor-handle unselectable">
    <div>.
      <br>.
      <br>.</div>
  </div> -->

  <div class="cloud-ide-editor" id="cloud-ide-editor"></div>
  <% if(env === 'development') { %>
    <script src="http://localhost:9000/webpack-dev-server.js"></script>
    <% } %>
      <script src="./lib/emmet.js"></script>
      <script src="./lib/ace-min-noconflict/ace.js"></script>
      <script src="./lib/jscolor/jscolor.js"></script>
      </script>
      <script src="/assets/js/main.build<% if(env === 'production') { %>.<%= hash %><% } %>.js"></script>
</body>


</html>
